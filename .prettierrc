{
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babel",
    "proseWrap": "always",
    "endOfLine":"auto",
    "overrides": [
        {
          "files": "*.scss",
          "options": {
            "parser": "css"
          }
        }
      ]
}
